{
  "name": "react-autocomplete",
  "version": "1.5.9",
  "description": "Accessible, extensible, Autocomplete for React.js",
  "main": "./build/lib/Autocomplete.js",
  "files": [
    "build/lib/index.js",
    "build/lib/Autocomplete.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-autocomplete.git"
  },
  "homepage": "https://github.com/reactjs/react-autocomplete",
  "bugs": "https://github.com/reactjs/react-autocomplete/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "generate-readme": "node bin/generate-readme.js",
    "gh-pages": "bin/gh-pages.sh",
    "release": "bin/release.sh",
    "build": "npm run build:component && npm run build:examples",
    "build:component": "bin/build-component.sh",
    "build:examples": "bin/build-examples.sh",
    "test": "npm run test:lint && npm run test:jest",
    "test:lint": "eslint lib examples --ignore-pattern=examples/__build__/**",
    "test:jest": "jest",
    "coverage": "jest --coverage",
    "start": "bin/start.sh"
  },
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "6.1.2",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "browserify-hmr": "^0.3.5",
    "concurrently": "^3.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^2.13.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^6.10.3",
    "exorcist": "^0.4.0",
    "jest": "^20.0.4",
    "lodash.flow": "^3.5.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "react": "^15.5.4",
    "react-docgen": "^2.15.0",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "st": "^1.2.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0",
    "youemdee": "^1.0.0"
  },
  "tags": [
    "react",
    "autocomplete",
    "combobox",
    "a11y"
  ],
  "keywords": [],
  "dependencies": {
    "dom-scroll-into-view": "1.0.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0",
    "react-dom": "^0.14.7 || ^15.0.0-0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ]
  }
}
